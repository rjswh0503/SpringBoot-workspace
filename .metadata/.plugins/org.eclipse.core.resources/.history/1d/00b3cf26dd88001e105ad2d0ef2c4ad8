<?xml version="1.0" encoding="UTF-8"?>


<mapper>
	<!-- id값에다가 전달해줄 메서드를 지정해줌
		예를들어 getAllProducts 메서드에 전달해주고 싶으면 id 값에 getAllProducts를 넣어주면 됨 
		resultType 전달해줄 장소 model 객체로 매핑해서 반환하도록 설정한 것
	 -->
	<select id="getAllProducts" resultType="com.kh.springdb.model">
		SELECT * FROM products
	</select>
</mapper>